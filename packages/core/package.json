{
  "name": "@jpoffice/core",
  "version": "0.1.0",
  "description": "JPOffice core - convenience re-export bundle",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src",
    "test": "vitest run --passWithNoTests",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@jpoffice/model": "workspace:*",
    "@jpoffice/engine": "workspace:*",
    "@jpoffice/layout": "workspace:*",
    "@jpoffice/renderer": "workspace:*",
    "@jpoffice/react": "workspace:*",
    "@jpoffice/docx": "workspace:*",
    "@jpoffice/pdf": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.6.0"
  }
}
