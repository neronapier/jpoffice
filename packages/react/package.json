{
  "name": "@jpoffice/react",
  "version": "0.1.1",
  "description": "JPOffice React components - editor, viewer, toolbar, hooks",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@jpoffice/engine": "workspace:*",
    "@jpoffice/layout": "workspace:*",
    "@jpoffice/model": "workspace:*",
    "@jpoffice/renderer": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "@jpoffice/docx": "workspace:*",
    "@jpoffice/pdf": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@jpoffice/docx": { "optional": true },
    "@jpoffice/pdf": { "optional": true }
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "jsdom": "^28.1.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  }
}
